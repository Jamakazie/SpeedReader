{
	"manifest_version": 2,

	"name" : "jSpeed Reader",
	"description" : "Speed read: reading fast and cutomizing to fit your needs",
	"version": "1.0",

	"permissions": [
		"contextMenus"
	],

	"icons":{
		"16": "icon16.png"
	},
	
	"content_scripts": [
		{
		"matches": ["http://*/*"],
		"js": ["speedread.js"]
		}
	],
	"background": {
		"scripts": ["context.js"]
	}
}
